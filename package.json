{
  "name": "timeline-builder",
  "version": "1.0.0",
  "description": "Compare and merge date ranges and create a timeline",
  "main": "index.js",
  "types": "index.d.ts",  
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc && webpack",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepub": "copy package.json dist && copy README.md dist",
    "pub": "cd dist & yarn publish",
    "postpub": "cd dist & del package.json & del README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimesoftware/timeline-builder.git"
  },
  "author": "Dime Software",
  "license": "MIT",
  "keywords": [
    "timeline",
    "daterange",
    "buffering",
    "buffer",
    "merge",
    "datecompare"
  ],
  "bugs": {
    "url": "https://github.com/dimesoftware/timeline-builder/issues"
  },
  "homepage": "https://github.com/dimesoftware/timeline-builder#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
